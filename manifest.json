{
    "manifest_version": 2,
    "name": "TotalProductivity",
    "description": "Best chrome extension that helps maximize user productivity",
    "version": "1.0.0",
    "background": {
        "scripts": ["script.js"],
        "persistent" : false
    },
    "browser_action" : {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}