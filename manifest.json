{
    "manifest_version": 2,
    "name": "TotalProductivity",
    "description": "Best chrome extension that helps maximize user productivity",
    "version": "1.0.0",
    "background": {
        "scripts": ["script.js", "javascript/jquery-3.4.1.min.js", "https://kit.fontawesome.com/7b773b220b.js", "jquery.fittext.js", ],
        "persistent" : false
    },
    "browser_action" : {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}