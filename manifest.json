{
    //Required
    "manifest_version": 2,
    "name": "TotalProductivity",
    "version": "1.0.0",
    //Reccommended
    "default_locale": "en",
    "description": "Best chrome extension that helps maximize user productivity",
    "icons":{},
    "background": {
        "scripts": ["script.js", "javascript/jquery-3.4.1.min.js", "javascript/fontawesome.js", "javascript/jquery.fittext.js"],
        "persistent" : false
    },
    "browser_action" : {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "certificateProvider",
        "cookies",
        "geolocation"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'"
}